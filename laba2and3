 puts("____________________________\n"); 
 printf("| %d * %d | %d + %d | %d - %d |\n", a, b, a, b, a, b); 
 puts("____________________________\n"); 
 printf("| %d\t| %d\t| %d\t|\n", a * b, a + b, a - b);


