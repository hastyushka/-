#define _CRT_SECURE_NO_DEPRECATE
#define ROW 9
#define COL 9

#include <stdio.h>
#include <locale.h>
#include <math.h>

void main(void){
	int row, col;
	for (col = 1; col <= COL; ++col){
		for (row = 1; row <= ROW; ++row){
			printf("%d x %d = %d", col, row, col * row);
			printf("\t");
		}
		printf("\n");
	}

}
